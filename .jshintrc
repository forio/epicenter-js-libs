{
    "browser": true,
    "node": true,
    "curly": true,
    "camelcase": true,
    "eqeqeq": true,
    "es3": false,
    "forin": true,
    "freeze": true,
    "immed": true,
    "indent": 4,
    "latedef": "nofunc",
    "newcap": true,
    "noarg": true,
    "noempty": false,
    "nonbsp": true,
    "nonew": false,
    "plusplus": false,
    "quotmark": "single",
    "undef": true,
    "unused": "vars",
    "strict": true,
    "trailing": true,
    "eqnull": true,
    "globals": {
        "$": false,
        "jQuery": false
    }
}
