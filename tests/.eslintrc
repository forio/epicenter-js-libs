{
    "extends": ["../.eslintrc"],
    "env": {
        "mocha": true
    },
    "globals": {
        "expect": false,
        "sinon": false,
        "F": false,
        "$": false,
        "_": false,
        "should": false
    },
    "rules": {
        "no-alert": 0,
        "no-magic-numbers": 0,
        "one-var": 0,
        "no-unused-expressions": 0
    }
}